{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": ".",
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "composite": true
  },
  "include": [
    "nodes/**/*.ts",
    "credentials/**/*.ts",
    "index.ts"
  ]
}
